# AI Usage Dashboard Configuration
# Copy this file to .env and fill in your values

# Ollama Cloud - get from https://ollama.com/settings/keys
OLLAMA_API_KEY=

# Ollama local instance (default: http://localhost:11434)
OLLAMA_LOCAL_HOST=http://localhost:11434

# Ollama session cookie for usage scraping (from ollama.com/settings)
# DevTools -> Application -> Cookies -> ollama.com -> __Secure-session
OLLAMA_SESSION_COOKIE=

# Claude Pro - sessionKey cookie from claude.ai
# DevTools -> Application -> Cookies -> claude.ai -> sessionKey
CLAUDE_SESSION_KEY=

# ChatGPT Plus - OAuth access token
# Auto-reads from ~/.codex/auth.json (Codex CLI)
# Or copy from DevTools -> Network -> Authorization header on chatgpt.com
CHATGPT_ACCESS_TOKEN=

# Copilot Pro - GitHub PAT (classic) with read:user scope
# Create at https://github.com/settings/tokens
GITHUB_TOKEN=

# Z.AI GLM Coding Plan - auth token from api.z.ai
# Get from Z.AI dashboard or coding plugin settings
ZAI_AUTH_TOKEN=

# Network binding (secure default keeps server local only)
# Use 0.0.0.0 only when you intentionally need remote access
BIND_HOST=127.0.0.1

PORT=3456
